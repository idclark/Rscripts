rm(list=ls())

library(XML)
library(ggplot2)
library(reshape)

weburl <- "http://results.public.chicagomarathon.com/2011/index.php?page=1&content=list&event=MAR&lang=EN&num_results=1000&pid=list&search[age_class]=&search[sex]=M&search_sort_order=ASC&top_results=3&type=list"

tables <- readHTMLTable(weburl)
n.rows <- unlist(lapply(tables, function(t) dim(t)[1]))

times <- tables[[which.max(n.rows)]]
